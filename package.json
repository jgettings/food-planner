{
  "name": "food-planner",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "color-convert": "^2.0.0",
    "cross-fetch": "^3.0.2",
    "express": "^4.16.4",
